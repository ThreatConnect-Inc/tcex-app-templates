{
  "allowOnDemand": true,
  "appId": "230b94ee-2144-588c-94b7-711724ab1d75",
  "category": "Utility",
  "displayName": "JSON Pretty",
  "features": [
    "aotExecutionEnabled",
    "appBuilderCompliant",
    "fileParams",
    "redisPasswordSupport",
    "runtimeVariables"
  ],
  "languageVersion": "3.11",
  "listDelimiter": "|",
  "minServerVersion": "7.2.0",
  "note": "Take raw JSON string and output \"pretty\" version with provide indent level. Optionally sort the keys.\n",
  "params": [
    {
      "label": "JSON Data",
      "name": "json_data",
      "note": "The JSON data to be formatted.",
      "playbookDataType": [
        "KeyValue",
        "KeyValueArray",
        "String",
        "StringArray",
        "TCEntity",
        "TCEntityArray"
      ],
      "required": true,
      "sequence": 1,
      "type": "String",
      "validValues": [
        "${TEXT}"
      ]
    },
    {
      "default": "4",
      "label": "Indent",
      "name": "indent",
      "note": "The number of spaces to use for indention (default: 4).",
      "playbookDataType": [
        "String"
      ],
      "sequence": 2,
      "type": "String",
      "validValues": [
        "${TEXT}"
      ]
    },
    {
      "default": false,
      "label": "Sort Keys",
      "name": "sort_keys",
      "note": "Indicates whether keys should be sorted.",
      "sequence": 3,
      "type": "Boolean"
    }
  ],
  "playbook": {
    "outputVariables": [
      {
        "name": "json.pretty",
        "type": "String"
      }
    ],
    "type": "Utility"
  },
  "programLanguage": "PYTHON",
  "programMain": "run.py",
  "programVersion": "1.0.0",
  "runtimeLevel": "Playbook",
  "sdkVersion": "4.0.0"
}
