{
  "allowOnDemand": true,
  "appId": "d6e66fcc-8b15-5d91-baaf-1f2d753c6662",
  "category": "Service Trigger",
  "displayName": "Basic Service Trigger",
  "features": [
    "appBuilderCompliant",
    "fileParams",
    "redisPasswordSupport",
    "runtimeVariables",
    "secureParams"
  ],
  "languageVersion": "3.11",
  "listDelimiter": "|",
  "minServerVersion": "7.2.0",
  "note": "\n",
  "params": [
    {
      "label": "Example Service Input",
      "name": "service_input",
      "note": "An example input to the Trigger at the service level.",
      "required": true,
      "sequence": 1,
      "serviceConfig": true,
      "type": "String",
      "validValues": [
        "${TEXT}"
      ]
    },
    {
      "label": "Example Trigger Input",
      "name": "trigger_input",
      "note": "An example input to the Trigger at the playbook level.",
      "playbookDataType": [
        "String"
      ],
      "required": true,
      "sequence": 2,
      "type": "String",
      "validValues": [
        "${TEXT}"
      ]
    }
  ],
  "playbook": {
    "outputVariables": [
      {
        "name": "example.date_time",
        "type": "String"
      },
      {
        "name": "example.service_input",
        "type": "String"
      },
      {
        "name": "example.trigger_input",
        "type": "String"
      }
    ],
    "type": "Service Trigger"
  },
  "programLanguage": "PYTHON",
  "programMain": "run.py",
  "programVersion": "1.0.0",
  "runtimeLevel": "TriggerService",
  "sdkVersion": "4.0.1"
}
